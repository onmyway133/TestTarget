EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/Nimble" "$CONFIGURATION_BUILD_DIR/Quick" "$CONFIGURATION_BUILD_DIR/Sugar"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Nimble/Nimble.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Quick/Quick.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Sugar/Sugar.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Nimble" -framework "Quick" -framework "Sugar"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}/Pods
